{
  "name": "propspacex-user-service",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "build:only": "tsc && tsc-alias",
    "build": "npm run clean && npm run build:only",
    "prestart": "npm run build",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write .",
    "migration:generate": "tsx ./node_modules/typeorm/cli.js migration:generate -d src/config/data.source.ts",
    "migration:run": "tsx ./node_modules/typeorm/cli.js migration:run -d src/config/data.source.ts",
    "migration:revert": "tsx ./node_modules/typeorm/cli.js migration:revert -d src/config/data.source.ts",
    "migration:show": "tsx ./node_modules/typeorm/cli.js migration:show -d src/config/data.source.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fredrickray/propspacex-user-service.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fredrickray/propspacex-user-service/issues"
  },
  "homepage": "https://github.com/fredrickray/propspacex-user-service#readme",
  "description": "",
  "dependencies": {
    "@grpc/grpc-js": "^1.14.2",
    "@grpc/proto-loader": "^0.8.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "geoip-lite": "^1.4.10",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.2",
    "juice": "^11.0.1",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/geoip-lite": "^1.4.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "cross-env": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
